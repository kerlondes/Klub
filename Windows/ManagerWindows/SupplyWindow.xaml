<Window x:Class="Klub.Windows.SupplyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klub.Windows"
        mc:Ignorable="d"
        Title="Поставка товара" Height="153" Width="275" Background="#FFE2F0D9">
    <Grid>
        <Label Content="Товар" HorizontalAlignment="Left" Margin="34,15,0,0" VerticalAlignment="Top"/>
        <ComboBox Name="TovarComboBox" HorizontalAlignment="Left" Margin="118,17,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Количество" HorizontalAlignment="Left" Margin="34,52,0,0" VerticalAlignment="Top"/>
        <TextBox Name="QuantityTextBox" HorizontalAlignment="Left" Margin="118,57,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="На главную" HorizontalAlignment="Left" Margin="34,93,0,0" VerticalAlignment="Top" Width="69" Background="White" BorderBrush="#FF3E863B" Click="BackButton_Click"/>
        <Button Content="Поставить" Name="PostavkaButton" HorizontalAlignment="Left" Margin="117,93,0,0" VerticalAlignment="Top" Width="120" Background="White" BorderBrush="#FF186516" Click="PostavkaButton_Click"/>
    </Grid>
</Window>
