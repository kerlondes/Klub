<Window x:Class="Klub.Windows.ManagerWindows.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Klub.Windows.ManagerWindows"
        mc:Ignorable="d"
        Title="Окно менеджера" Height="246" Width="213" Background="#FFE2F0D9">
    <Grid>
        <Button Content="Формировать заказ" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="33" Width="189" BorderBrush="#FF226F1F" Background="White" Click="Button_Click"/>
        <Button Content="Редактировать заказы" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top" Height="33" Width="189" BorderBrush="#FF226F1F" Background="White" Click="Button_Click_1"/>
        <Button Content="Просмотреть остатки товаров" HorizontalAlignment="Left" Margin="10,109,0,0" VerticalAlignment="Top" Height="33" Width="189" BorderBrush="#FF226F1F" Background="White" Click="Button_Click_2"/>
        <Button Content="Добавить товары" HorizontalAlignment="Left" Margin="10,160,0,0" VerticalAlignment="Top" Height="33" Width="189" BorderBrush="#FF226F1F" Background="White" Click="Button_Click_3"/>

    </Grid>
</Window>
